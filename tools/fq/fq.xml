<tool id="fq" name="fq-show" version="0.0.1">
    <description>Summarise a fastq file</description>
    <requirements>
    </requirements>
    <stdio>
        <exit_code range="1:" />
    </stdio>
    <command><![CDATA[
        fq 2.fastq > 2.tabular
    ]]></command>
    <inputs>
        <param type="data" name="input1" format="fastq" />
    </inputs>
    <outputs>
        <data name="output1" format="tabular" from_work_dir="output.tabular" />
    </outputs>
    <tests>
        <test>
            <param name="input1" value="input.fastq"/>
            <output name="output1" file="output.tabular"/>
        </test>
    </tests>
    <help><![CDATA[
        help!
    ]]></help>
    <citations>
      <citation type="bibtex">Torsten</citation>
    </citations>
</tool>