1. checkBimodality : weight list


gRange <- as.character(unlist(strsplit(gsub(" ", "", "${GRanges}", ","), ",")))
    b <- unlist(strsplit(gsub(" ", "", "${IRanges}", ","), ";"))
    c <- gsub("\\[|\\]", "", b)
    d <- lapply(c, function(x){x <- as.numeric(unlist(strsplit(x, ",")))})
    e <- c()
    f <- c()
    for (i in seq(d)){ 
        e <- c(e, d[[i]][1])
        f <- c(f, d[[i]][2])
    }

    regions <- GRanges(gRange, IRanges(e,f))