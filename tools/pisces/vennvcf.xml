<?xml version="1.0"?>
<tool id="pisces_vennvcf" name="Pisces VennVcf" version="@TOOL_VERSION@+galaxy@VERSION_SUFFIX@" profile="18.01">
    <description>for intersection and disjoint segmentation of BCF</description>    
    <macros>
        <import>macros.xml</import>
    </macros>
    <expand macro="requirements"/>
    <expand macro="version_command"/>
    <command detect_errors="exit_code"><![CDATA[
## run
dotnet VennVcf
## required
--if $if1 $if2 ## todo
## common
-o 'results'
--consensus '$cp.consensus'
--debug true
## bam filtering
--minbq $bfo.minbq
--minmq $bfo.minmq
$bfo.filterduplicates
$bfo.pp
## variant calling
--minvq $vco.minvq
-c $vco.c
--minvf $vco.minvf
--targetlodfrequency $vco.targetlodfrequency
--vqfilter $vco.vqfilter
--vffilter $vco.vffilter
--gqfilter $vco.gqfilter
--mindpfilter $vco.mindpfilter
$vco.ssfilter
--nl $vco.nl
--ploidy $vco.ploidy
--diploidsnvgenotypeparameters $vco.diploidsnvgenotypeparameters
--diploidindelgenotypeparameters $vco.diploidindelgenotypeparameters
--adaptivegenotypeparameters_fromfile $vco.adaptivegenotypeparameters_fromfile
--adaptivegenotypeparameters_snvmodel $vco.adaptivegenotypeparameters_snvmodel
--adaptivegenotypeparameters_indelmodel $vco.adaptivegenotypeparameters_indelmodel
--adaptivegenotypeparameters_snvprior $vco.adaptivegenotypeparameters_snvprior
--adaptivegenotypeparameters_indelprior $vco.adaptivegenotypeparameters_indelprior
--sbmodel $vco.sbmodel 
--maxvq $vco.maxvq
--maxgq $vco.maxgq
--maxgp $vco.maxgp
--mingq $vco.mingq
--sbfilter $vco.sbfilter
--noisemodel $vco.noisemodel
--gender $vco.gender
--rmxnfilter $vco.rmxnfilter
--ncfilter $vco.ncfilter
## vcf writer
$vwo.gvcf
$vwo.crushvcf
$vwo.reportnocalls
$vwo.reportrccounts
$vwo.reporttscounts
$vwo.reportsuspiciouscoveragefraction

## postprocessing

    ]]></command>
    <inputs>
        <param argument="--if1" type="data" format="txt" label="Select first input files"/>
        <param argument="--if2" type="data" format="txt" label="Select second input files"/>
        <section name="co" title="Common options">
            <param argument="--consensus" type="data" format="" label="Select consensus file"/> td filetype
        </section>
        <section name="bfo" title="BAM filtering options">
            <param argument="--minbq" type="integer" min="" max="" value="" label="Set minimum basecall quality to use a base of a the read"/> --minbq, --minbasecallquality
            <param argument="--minmq" type="integer" min="" max="" value="" label="Set minimum map quality required to use a read"/> --minmq, --minmapquality
            <param argument="--filterduplicates" type="boolean" truevalue="--filterduplicates true" falsevalue="" label="Filter reads marked as duplicates?"/> --filterduplicates, --duplicatereadfilter
            <param argument="--pp" type="boolean" truevalue="--onlyuseproperpairs true" falsevalue="" label="Only use proper pairs?"/> --pp, --onlyuseproperpairs
        </section>
        <section name="vco" title="Variant calling options">                        
            <param argument="--minvq" type="integer" min="" max="" value="" label="MinimumVariantQScore to report variant"/> --minvq,   --minvariantqscore
            <param argument="-c" type="integer" min="" max="" value="" label="Minimum depth to call a variant" help=""/> -c, --mindp, --mindepth, --mincoverage
            <param argument="--minvf" type="float" min="" max="" value="" label="MinimumFrequency to call a variant" help=""/> --minvf, --minimumvariantfrequency, --minimumfrequency 
            <param argument="--targetlodfrequency, --targetvf" type="float" min="" max="" value="" label="Target Frequency to call a variant" help="Ie, to target a 5% allele frequency, we must call down to 2.6%, to capture that 5% allele 95% of the time. This parameter is used by the Somatic Genotyping Model"/>--targetlodfrequency, --targetvf
            <param argument="--vqfilter, --variantqualityfilter" type="integer" min="" max="" value="" label="FilteredVariantQScore to report variant as filtered" help=""/>--vqfilter, --variantqualityfilter
            <param argument="--vffilter, --minvariantfrequencyfilter" type="float" min="" max="" value="" label="FilteredVariantFrequency to report variant as filtered" help=""/> --vffilter, --minvariantfrequencyfilter
            <param argument="--gqfilter, --genotypequalityfilter" type="integer" min="" max="" value="" label="Filtered Genotype quality to report variant as filtered" help=""/> --gqfilter, --genotypequalityfilter
            <param argument="--mindpfilter, --mindepthfilter" type="integer" min="" max="" value="" label="FilteredLowDepth to report variant as filtered" help=""/> --mindpfilter, --mindepthfilter
            <param argument="--ssfilter" type="boolean" truevalue="--ssfilter true" falsevalue="" checked="true" label="Flag variants as filtered if coverage limited to one strand" help=""/> --ssfilter, --enablesinglestrandfilter
            <param argument="--nl" type="integer" min="" max="" value="" label="Overrides the noise level to used by the quality model with this value." help="By default, this is driven by the basecall filter."/> --nl, --noiselevelforqmodel
            <param argument="--ploidy" type="select" multiple="false" label="Set ploidy" help="To test drive the new adaptive model, try 'DiploidByAdaptiveGT'">
                <option value="somatic" selected="true">somatic</option>
                <option value="diploid">diploid</option>
            </param> 
            <param argument="--diploidsnvgenotypeparameters" type="text" value="0.2,0.7,0.8" label="diploidsnvgenotypeparameters" help="A, B, C"/>
            <param argument="--diploidindelgenotypeparameters" type="text" value="0.2,0.7,0.8" label="--diploidindelgenotypeparameters" help="A, B, C"/>
            <param argument="--adaptivegenotypeparameters_fromfile" type="data" format="" optional="true" label="--adaptivegenotypeparameters_fromfile" help=""/>
            <param argument="--adaptivegenotypeparameters_snvmodel" type="text" value="0.034,0.167,0.499,0.998" label="--adaptivegenotypeparameters_snvmodel" help="A,B,C,D"/>
            <param argument="--adaptivegenotypeparameters_indelmodel" type="text" value="0.037,0.443,0.905" label="--adaptivegenotypeparameters_indelmodel" help="A,B,C"/>
            <param argument="--adaptivegenotypeparameters_snvprior" type="text" value="0.729,0.044,0.141,0.087" label="--adaptivegenotypeparameters_snvprior" help="A,B,C,D"/>
            <param argument="--adaptivegenotypeparameters_indelprior" type="text" value="0.962,0.0266,0.0114" label="--adaptivegenotypeparameters_indelprior" help="A,B,C"/>
            <param argument="--sbmodel" type="text" value="" label="--sbmodel" help=""/>
            <param argument="--maxvq, --maxvariantqscore" type="integer" min="" max="" value="100" label="MaximumVariantQScore to cap output variant Qscores" help=""/> --maxvq, --maxvariantqscore
            <param argument="--maxgq, --maxgenotypeqscore" type="integer" min="" max="" value="100" label="Maximum genotype QScore to cap output genotype Qscores" help=""/>--maxgq, --maxgenotypeqscore
            <param argument="--maxgp, --maxgenotypeposteriorscore" type="integer" min="" max="" value="3000" label="Maximum genotype posterior score to cap output genotype posteriors" help=""/>--maxgp, --maxgenotypeposteriorscore
            <param argument="--mingq, --mingenotypeqscore" type="integer" min="" max="" value="0" label="Minimum genotype QScore to cap output genotype Qscores" help=""/> --mingq, --mingenotypeqscore
            <param argument="--sbfilter, --maxacceptablestrandbiasfilter" type="float" min="" max="" value="0.5" label="Strand bias cutoff" help=""/> --sbfilter, --maxacceptablestrandbiasfilter
            <param argument="--noisemodel" type="select" multiple="false" label="--noisemodel" help="">
                <option value="Window" selected="true">Window</option>
                <option value="Flat">Flat</option>
            </param>
            <param argument="--gender" type="select" label="Select gender">
                <option value="" selected="true">No</option>
                <option value="true">Male</option>
                <option value="false">Female</option>
            </param>
            <param argument="--rmxnfilter" type="text" value="R5x9,F=20" label="rmxnfilter" help="M,N,F. Comma-separated list of integers indicating max length of the repeat section (M), the minimum number of repetitions of that repeat (N), to be applied if the variant frequency is less than (F)."/>--rmxnfilter
            <param argument="--ncfilter" type="float" min="" max="" value="" label="No-call rate filter" help=""/>--ncfilter
        </section>
        <section name="vwo" title="VCF writer options">
            <param argument="--gvcf" type="boolean" truevalue="--gvcf true" falsevalue="--gvcf false" checked="true" label="Output gVCF files?" help=""/>
            <param argument="--crushvcf" type="boolean" truevalue="--crushvcf true" falsevalue="--crushvcf false" checked="true" label="To crush vcf output to one line per loci" help=""/>--crushvcf
            <param argument="--reportnocalls" type="boolean" truevalue="--reportnocalls true" falsevalue="--reportnocalls false" label="--reportnocalls" help=""/>
            <param argument="--reportrccounts" type="boolean" truevalue="--reportrccounts true" falsevalue="--reportrccounts true" checked="true" label="Report collapsed read count" help="When BAM files contain X1 and X2 tags, output read counts for duplex-stitched, duplex-nonstitched, simplex-stitched, and simplex-nonstitched.  'true' or  'false'. default, false"/>
            <param argument="--reporttscounts" type="boolean" truevalue="" falsevalue="" checked="true" label="Report collapsed read count by different template strands" help="Conditional on ReportRcCounts, output read counts for duplex-stitched, duplex-nonstitched, simplex-forward-stitched, simplex-forward-nonstitched, simplex-reverse-stitched, simplex-reverse-nonstitched.  'true' or 'false'. default, false"/>
            <param argument="--reportsuspiciouscoveragefraction" type="boolean" truevalue="" falsevalue="" checked="true" label="Report fraction of total coverage" help="(for spanning variants, this is start + end coverage, so up to double the reported coverage) that is 'suspicious' i.e. unanchored and bearing some resemblance to an insertion at that site. 'true' or 'false'. default, false"/>
        </section>        
        <section name="oo" title="Output options">
            <param name="out" type="select" multiple="true" optional="false" label="Select output file(s)" help="">
                <option value="" selected="true"></option>
                <option value=""></option>
                <option value="log">Log</option>
            </param>            
        </section>
    </inputs>
    <outputs>

    </outputs>
    <tests>
        <!-- #1 default -->

        <!-- #2 -->
    
    </tests>
