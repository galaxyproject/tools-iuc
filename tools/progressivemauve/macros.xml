<?xml version="1.0"?>
<macros>
  <xml name="requirements">
    <requirements>
      <requirement version="1.2.0">mauvealigner</requirement>
      <yield/>
    </requirements>
  </xml>
  <token name="@WRAPPER_VERSION@">2015_02_13+galaxy0</token>
  <xml name="stdio">
    <stdio>
      <!-- Anything other than zero is an error -->
      <exit_code range="1:"/>
      <exit_code range=":-1"/>
      <!-- In case the return code has not been set propery check stderr too -->
      <regex match="Error:"/>
      <regex match="Exception:"/>
    </stdio>
  </xml>
  <xml name="citation">
      <citations>
          <citation type="doi">10.1371/journal.pone.0011147</citation>
      </citations>
  </xml>
</macros>
