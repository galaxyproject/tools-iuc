<?xml version="1.0"?>
<data_managers>
    <data_manager tool_file="data_manager/data_manager_genomic_super_signature_ravs.xml" id="data_manager_genomic_super_signature_ravs">
        <data_table name="genomic_super_signature_ravs">
            <output>
                <column name="value" />
                <column name="dbkey" />
                 <column name="version" />
                <column name="name" />
                <column name="path" output_ref="out_file" >
                    <move type="directory" relativize_symlinks="True">
                        <!-- <source>${path}</source>--> <!-- out_file.extra_files_path is used as base by default --> <!-- if no source, eg for type=directory, then refers to base -->
                        <target base="${GALAXY_DATA_MANAGER_DATA_PATH}">${dbkey}/genomic_super_signature_ravs/${version}/${value}</target>
                    </move>
                    <value_translation>${GALAXY_DATA_MANAGER_DATA_PATH}/${dbkey}/genomic_super_signature_ravs/${version}/${value}/${path}</value_translation>
                    <value_translation type="function">abspath</value_translation>
                </column>
            </output>
        </data_table>
    </data_manager>
</data_managers>
