<?xml version="1.0"?>
<data_managers>
    <data_manager tool_file="data_manager/celltypist_model_downloader.xml" id="data_manager_celltypist_models">
        <data_table name="celltypist_models">
            <output>
                <column name="value" />
                <column name="date" />
                <column name="path" output_ref="out_file">
                    <move type="file" relativize_symlinks="True">
                        <target base="${GALAXY_DATA_MANAGER_DATA_PATH}/celltypist" />
                    </move>
                    <value_translation>${GALAXY_DATA_MANAGER_DATA_PATH}/celltypist</value_translation>
                    <value_translation type="function">abspath</value_translation>
                </column>
            </output>
        </data_table>
    </data_manager>
</data_managers>
