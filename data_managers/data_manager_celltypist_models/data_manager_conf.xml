<data_managers>
    <data_manager tool_file="data_manager/celltypist_model_downloader.xml" id="celltypist_model_downloader">
        <data_table name="celltypist_models">
            <output>
                <column name="model" />
                <column name="details" />
                <column name="date" />
                <column name="path" output_ref="out_file">
                    <move type="directory" relativize_symlinks="True">
                        <target base="${GALAXY_DATA_MANAGER_DATA_PATH}">celltypist/${model}.pkl</target>
                    </move>
                    <value_translation>${GALAXY_DATA_MANAGER_DATA_PATH}/celltypist/${model}.pkl</value_translation>
                    <value_translation type="function">abspath</value_translation>
                </column>
                <column name="path" />
            </output>
        </data_table>
    </data_manager>
</data_managers>