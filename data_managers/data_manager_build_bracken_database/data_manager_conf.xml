<data_managers>
    <data_manager tool_file="data_manager/bracken_build_database.xml" id="bracken_build_database" version="2.6+galaxy0">
        <data_table name="bracken_databases">
            <output>
                <column name="value"/>
                <column name="name"/>
                <column name="path" output_ref="out_file">
                    <move type="directory">
                        <source>${path}</source>
                        <target base="${GALAXY_DATA_MANAGER_DATA_PATH}">bracken_databases/${path}</target>
                    s</move>
                    <value_translation>${GALAXY_DATA_MANAGER_DATA_PATH}/braken_databases/${path}</value_translation>
                    <value_translation type="function">abspath</value_translation>
                </column>
            </output>
        </data_table>
    </data_manager>
</data_managers>
