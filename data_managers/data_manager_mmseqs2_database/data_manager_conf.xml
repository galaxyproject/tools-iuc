<data_managers>
    <data_manager tool_file="data_manager/data_manager_mmseqs2_download.xml" id="checkv_build_database">
        <data_table name="mmseqs2">
            <output>
                <column name="value"/>
                <column name="name"/>
                <column name="path" output_ref="out_file">
                    <move type="directory">
                        <source>${path}</source>
                        <target base="${GALAXY_DATA_MANAGER_DATA_PATH}">mmseqs2/${path}</target>
                    </move>
                    <value_translation>${GALAXY_DATA_MANAGER_DATA_PATH}/mmseqs2/${path}</value_translation>
                    <value_translation type="function">abspath</value_translation>
                </column>
                <column name="version"/>
            </output>
        </data_table>
    </data_manager>
</data_managers>